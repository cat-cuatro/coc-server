  },
  "author": "Michael Heyman",
  "license": "ISC",
  "dependencies": {
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "db-migrate-sqlite3": "^0.4.0",
    "express": "^4.17.1",
    "pg-promise": "^10.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "decache": "^4.5.1",
    "eslint": "^6.6.0",
    "husky": "^3.0.9",
    "mocha": "^6.2.2",
    "node-mocks-http": "^1.8.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "proxyquire": "^2.1.3",
    "sinon": "^7.5.0",
